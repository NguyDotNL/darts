{
  "name": "darts",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "npm run serve:dev",
    "serve:dev": "vue-cli-service serve --open --mode development",
    "serve:qa": "vue-cli-service serve --open --mode qa",
    "serve:prod": "vue-cli-service serve --open --mode production",
    "build": "npm run build:prod",
    "build:dev": "vue-cli-service build --mode development",
    "build:qa": "vue-cli-service build --mode qa",
    "build:prod": "vue-cli-service build --mode production",
    "fill-db:dev": "node -r dotenv/config ./scripts/generate-test-data-new.js dotenv_config_path=./.env.development",
    "fill-db:qa": "node -r dotenv/config ./scripts/generate-test-data-new.js dotenv_config_path=./.env.qa",
    "fill-db:prod": "node -r dotenv/config ./scripts/generate-test-data-new.js dotenv_config_path=./.env.production",
    "lint": "vue-cli-service lint",
    "t": "npm run test",
    "test": "jest --watch",
    "test:ci": "jest --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "dependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.11.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/vue": "^5.1.0",
    "apexcharts": "^3.21.0",
    "babel-core": "^7.0.0-bridge.0",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "eslint-plugin-jest": "^24.0.2",
    "firebase": "^7.22.1",
    "jest": "^26.4.2",
    "lodash": "^4.17.20",
    "minimist": "^1.2.5",
    "moment": "^2.29.1",
    "npm-check": "^5.9.2",
    "tailwindcss": "^1.8.10",
    "uuid": "^8.3.1",
    "vue": "^2.6.11",
    "vue-apexcharts": "^1.6.0",
    "vue-moment": "^4.1.0",
    "vue-router": "^3.2.0",
    "vuetify": "^2.2.11"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.6",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "eslint": "^6.8.0",
    "eslint-plugin-json": "^2.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "jest-vue": "^0.8.2",
    "lint-staged": "^10.4.0",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "~2.0.7",
    "vue-jest": "^3.0.7",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      "^.+\\.js$": "babel-jest",
      ".*\\.vue$": "<rootDir>/node_modules/vue-jest"
    },
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
